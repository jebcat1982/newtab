{
  "name": "Tekkub Newtab",
  "description": "My newtab page",
  "version": "0.19",
  "manifest_version": 2,
  "icons": {
    "128": "img/icon-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "home.html"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "storage",
    "https://api.parse.com/*"
  ],
  "browser_action": {
    "default_icon": {
      "19": "img/icon-128.png"
    },
    "default_title": "Loading Dropbox..."
  },
  "background": {
    "scripts": [
      "vendor/jquery.min.js",
      "vendor/dropbox-datastores.js",
      "bower_components/firebase/firebase.js",
      "js/models/browser-action.js",
      "js/models/settings.js",
      "js/models/storage-dropbox.js",
      "js/models/storage-firebase.js",
      "js/models/storage.js",
      "js/background.js"
    ]
    // "persistent": false
  },
  "web_accessible_resources": [
    "chrome_oauth_receiver.html"
  ]
}
